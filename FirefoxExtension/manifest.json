{
    "manifest_version": 3,
    "name": "My Font Changer Extension",
    "version": "1.0",
    "description": "Change the font of web pages.",
    "host_permissions": ["<all_urls>, activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    }
  }