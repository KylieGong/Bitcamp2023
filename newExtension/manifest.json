{
    "manifest_version": 3,
    "name": "Custom Font",
    "version": "1.0",
    "description": "Allows the user to change the font style and size of a website.",
    "permissions": ["activeTab"],
    "background": {
      "service_worker": "script.js"
    },
    "action": {
      "default_title": "Custom Font",
      "default_icon": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }