{
    "name": "Change Font Extension",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ],
    "options_ui": {
      "page": "options.html",
      "open_in_tab": true
    },
    "permissions": [
        "contextMenus",
        "storage"
      ]
  }