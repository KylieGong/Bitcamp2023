{
    "manifest_version": 3,
    "name": "Focus Mode",
    "description": "Enable focus mode on Chrome's official Extensions and Chrome Web Store documentation.",
    "version": "1.0",
    "action" : {
        "default_popup" : "startup_menu.html"
    },
    "permissions" : [
        "notifications",
        "activeTab"
    ],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js":["js/jquery-3.1.1.js", "textScript.js"],
          "run_at": "document_end"
    
        }
      ]
}